<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:bottom="2dp" android:left="0dp" android:right="2dp" android:top="2dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="0.6dp" android:color="@color/black" />

            <solid android:color="#A8FFAB" />
            <gradient
                android:type="linear"
                android:startColor="#FFD382"
                android:centerColor="#FFBF47"
                android:endColor="#FFB11F"
                android:angle="180" />

            <corners android:radius="20dp"/>
        </shape>
    </item>
</layer-list>

